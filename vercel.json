{
  "version": 2,
  "buildCommand": "cd Adaptive-Trace-Gamification && npm install && npm run build",
  "outputDirectory": "Adaptive-Trace-Gamification/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/attempts",
      "destination": "/api/attempts"
    },
    {
      "source": "/api/health",
      "destination": "/api/health"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
