{
  "version": 2,
  "buildCommand": "cd Adaptive-Trace-Gamification && npm install && npm run build",
  "outputDirectory": "Adaptive-Trace-Gamification/dist",
  "framework": "vite",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
